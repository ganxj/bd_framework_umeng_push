<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--支付宝信息-->
    <bean id="umengConfig" class="com.umeng.push.service.Impl.PushImpl" >
        <constructor-arg index="0" value="${umeng.backurl}"/>
        <constructor-arg index="1" value="${umeng.appkey}"/>
        <constructor-arg index="2" value="${umeng.app_master_secret}"/>
    </bean>


</beans>